{
  "compilerOptions": {
    "target": "ES2017",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "baseUrl":".",
    "paths": {
      "@/*": ["./*"],
      "@public/*": ["./public/*"],
      "@src/*": ["./src/*"],
      "@config/*": ["./src/config/*"],
//! App
      "@app/*": ["./src/app/*"],
      "@api/*": ["./src/app/api/*"],
      "@accounts/*": ["./src/app/accounts/*"],
        "@transactions/*": ["./src/app/accounts/transactions/*"],
      "@calendar/*": ["./src/app/calendar/*"],
        "@events/*": ["./src/app/calendar/events/*"],
      "@dashboard/*": ["./src/app/dashboard/*"],
      "@goals/*": ["./src/app/goals/*"],
      "@inventories/*": ["./src/app/inventories/*"],
        "@items/*": ["./src/app/inventories/items/*"],
      "@products/*": ["./src/app/products/*"],
        "@movements/*": ["./src/app/products/movements/*"],
      "@reports/*": ["./src/app/reports/*"],
      "@tasks/*": ["./src/app/tasks/*"],
      "@members/*": ["./src/app/members/*"],
        "@sessions/*": ["./src/app/members/sessions/*"],
//! Shared
      "@shared/*": ["./src/shared/*"],
      "@desks/*": ["./src/shared/desks/*"],
        "@categories/*": ["./src/shared/desks/categories/*"],
        "@folders/*": ["./src/shared/desks/folders/*"],
      "@home/*": ["./src/shared/home/*"],
      "@users/*": ["./src/shared/users/*"],
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts"],
  "exclude": ["node_modules"]
}

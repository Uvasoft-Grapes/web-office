{
  "compilerOptions": {
    "target": "ES2017",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "baseUrl":".",
    "paths": {
      "@/*": ["./*"],
      "@public/*": ["./public/*"],
      "@src/*": ["./src/*"],
      "@app/*": ["./src/app/*"],
      "@api/*": ["./src/app/api/*"],
      "@config/*": ["./src/config/*"],
      "@shared/*": ["./src/shared/*"],
      "@domains/*": ["./src/domains/*"],
      "@accounts/*": ["./src/domains/accounts/*"],
        "@transactions/*": ["./src/domains/accounts/transactions/*"],
      "@calendar/*": ["./src/domains/calendar/*"],
        "@events/*": ["./src/domains/calendar/events/*"],
      "@dashboard/*": ["./src/domains/dashboard/*"],
      "@desks/*": ["./src/domains/desks/*"],
        "@categories/*": ["./src/domains/desks/categories/*"],
        "@folders/*": ["./src/domains/desks/folders/*"],
      "@goals/*": ["./src/domains/goals/*"],
      "@home/*": ["./src/domains/home/*"],
      "@inventories/*": ["./src/domains/inventories/*"],
        "@items/*": ["./src/domains/inventories/items/*"],
      "@products/*": ["./src/domains/products/*"],
        "@movements/*": ["./src/domains/products/movements/*"],
      "@reports/*": ["./src/domains/reports/*"],
      "@tasks/*": ["./src/domains/tasks/*"],
      "@users/*": ["./src/domains/users/*"],
        "@sessions/*": ["./src/domains/users/sessions/*"],
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts"],
  "exclude": ["node_modules"]
}
